<%= form_with model: company, url: companys_path, method: :post do |f| %>

<table>
    <tr>
      <td> <%= f.label :name, "会社名" %></td>
    </tr>
    <tr>
      <td><%= f.text_field :name, placeholder: "株式会社A" %></td>
    </tr>
    <tr>
      <td><%= f.submit '新規登録' %></td>
    </tr>
</table>
<% end %>